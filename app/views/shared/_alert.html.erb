<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "3000Labs Portfolio", sticky: false)%>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "3000Labs Portfolio", sticky: false)%>
<% else flash[:notice] %>
  <%= js add_gritter(flash[:notice], title: "3000Labs Portfolio", sticky: false)%>
<% end %>
